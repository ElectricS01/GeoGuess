{
    "name": "geoguess",
    "version": "1.0.0",
    "description": "GeoGuess is an open-source geography game with Google Map StreetView.",
    "author": "Bilel Jegham and others",
    "scripts": {
        "serve": "vite serve",
        "dev": "vite dev",
        "build": "vite build",
