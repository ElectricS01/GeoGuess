        "axios": "^0.21.4",
        "axios-cache-adapter": "^2.7.3",
        "dotenv": "^16.3.1",
        "firebase": "^8.10.1",
        "jest-google-maps-mock": "^1.0.7",
        "jsdom": "^22.1.0",
        "patch-package": "^8.0.0",
        "random-position-in-polygon": "^1.0.6",
        "register-service-worker": "^1.7.2",
        "vite": "^4.5.0",
